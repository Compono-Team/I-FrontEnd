{
  "name": "i-frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@tanstack/react-query": "^5.12.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "axios": "^1.6.2",
    "env-cmd": "^10.1.0",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "react-ga4": "^2.1.0",
    "react-router-dom": "^6.21.1",
    "react-scripts": "5.0.1",
    "sass": "^1.69.5",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0",
    "zustand": "^4.4.7"
  },
  "scripts": {
    "start:dev": "env-cmd -e ,development craco start",
    "start:prod": "env-cmd -e ,production craco start",
    "build:dev": "env-cmd -e ,development,build craco build",
    "build:prod": "env-cmd -e ,production,build craco build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint src/ --ext .tsx,.ts",
    "lint:stylelint": "stylelint \"src/**/*.scss\" --config .stylelint.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/sha256": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^4.0.0"
  }
